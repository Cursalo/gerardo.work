{
  "rewrites": [
    {
      "source": "/project/([^/]+)$",
      "destination": "/index.html"
    },
    {
      "source": "/((?!api|_next|_static|_vercel|favicon.ico|public|projects).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/projects/(.*)\\.webp$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        },
        {
          "key": "Content-Type",
          "value": "image/webp"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Accept"
        }
      ]
    },
    {
      "source": "/projects/(.*)\\.jpg$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        },
        {
          "key": "Content-Type",
          "value": "image/jpeg"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Accept"
        }
